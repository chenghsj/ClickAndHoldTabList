{
	"manifest_version": 3,

	"name": "Tab Menu Node",
	"description": "the tab list will appear after double click middle button",
	"version": "1.0",
	"icons": {
		"128": "icon2.png"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"options_ui":{
		"page": "options/options.html",
		"open_in_tab": false
	},
	"web_accessible_resources": [
		{
			"resources": ["inject/createTabMenu.js", "inject/nodeType.js"],
			"matches": ["<all_urls>"]
		}
	],
	"minimum_chrome_version": "92",
	"action": {
		"default_icon": "icon2.png"
	},
	"permissions": ["tabs", "scripting", "storage"],
	"host_permissions": ["<all_urls>"]
}
